<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sbs.lyb.pp.dao.BoardDao">
	<insert id="createGroupBoard">
		INSERT INTO `board`
		SET regDate = NOW(),
		updateDate = NOW(),
		`name` = #{code},
		`code` = #{code}
	</insert>
	
	<delete id="deleteBoard">
		DELETE FROM `board`
		WHERE `code` = #{code}
	</delete>
</mapper>